{
  "name": "quadratic-rust-layout",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "CC=clang AR=llvm-ar wasm-pack build --target web --out-dir ../../quadratic-client/src/app/web-workers/rustRendererWebWorker/layout-pkg --weak-refs",
