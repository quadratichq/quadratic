{
  "editor.formatOnSave": true,
  "cSpell.words": [
    "actix",
    "bigdecimal",
    "bindgen",
    "dashmap",
    "dbgjs",
    "dcell",
    "ddimaria",
    "dgraph",
    "dotenv",
    "endregion",
    "Fuzzysort",
    "gramm",
    "grammarly",
    "Hasher",
    "healthcheck",
    "healthchecks",
    "hljs",
    "htmlescape",
    "indexmap",
    "indicies",
    "itertools",
    "jwks",
    "MDSL",
    "micropip",
    "msdf",
    "nonblank",
    "Northbridge",
    "openai",
    "opensans",
    "peekable",
    "pixi",
    "pixiapp",
    "pulumi",
    "pyimport",
    "rects",
    "reqwest",
    "shadcn",
    "Signin",
    "smallpop",
    "Southborough",
    "szhsin",
    "thiserror",
    "unspill",
    "vals",
    "websockets",
    "Westborough"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.alwaysShowStatus": true,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "rust-analyzer.linkedProjects": [
    "./Cargo.toml",
    "./quadratic-files/Cargo.toml",
    "./quadratic-core/Cargo.toml",
    "./quadratic-multiplayer/Cargo.toml",
    "./quadratic-rust-client/Cargo.toml"
  ],
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "files.associations": {
    "*.grid": "json"
  },
  "jest.rootPath": "quadratic-client",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "spellright.language": ["en-GB-10-1."],
  "spellright.documentTypes": ["markdown", "latex", "plaintext", "python", "rust", "typescript"]
}
