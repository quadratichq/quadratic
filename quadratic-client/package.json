  "main": "index.html",
  "scripts": {
    "preview": "npm run build && vite preview --port=3000",
    "start": "export VITE_VERSION=$npm_package_version && export NODE_OPTIONS=--max-old-space-size=16384 && npm run download:pyodide && vite dev",
    "start:no-hmr": "npm run download:pyodide && npm run build && vite preview --port 3000",
    "build": "export VITE_VERSION=$npm_package_version && export NODE_OPTIONS=--max-old-space-size=16384 && vite build",
    "build:docker": "export VITE_VERSION=$npm_package_version && export NODE_OPTIONS=--max-old-space-size=16384 && vite build --mode docker",
    "build:wasm": "npm run build:wasm:javascript && npm run build:wasm:nodejs && npm run build:wasm:types",
    "build:wasm:types": "cd .. && cd quadratic-core && CC=clang AR=llvm-ar cargo run --bin export_types",
    "build:wasm:javascript": "cd .. && cd quadratic-core && CC=clang AR=llvm-ar wasm-pack build --target web --out-dir ../quadratic-client/src/app/quadratic-core --weak-refs",
    "build:wasm:nodejs": "cd .. && cd quadratic-core && CC=clang AR=llvm-ar wasm-pack build --target nodejs --out-dir ../quadratic-client/src/app/quadratic-core/__mocks__ --weak-refs",
    "build:wasm:javascript:dev": "cd .. && cd quadratic-core && CC=clang AR=llvm-ar wasm-pack build --target web --out-dir ../quadratic-client/src/app/quadratic-core --weak-refs --dev",
    "start:production": "serve build",
    "test:all": "concurrently \"npm run test:unit:ci\" \"npm run test:e2e\"",
    "test:unit": "vitest",
    "test:unit:dev": "npm run build:wasm:nodejs && npm run test:unit -- --watchAll",
    "test:unit:ci": "npm run test:unit -- --ci --runInBand",
    "test:wasm": "cd .. && cd quadratic-core && cargo test --workspace",
    "test:vite": "vitest",
    "dev": "concurrently \"BROWSER=none npm start\" \"electron .\"",
    "electron": "electron .",
    "lint:ts": "tsc --noemit",
    "lint:ts:write": "npm run lint:prettier:write && tsc --noemit && npm run lint:eslint:write",
    "lint:eslint": "eslint --ext .ts,.tsx src",
    "lint:eslint:write": "eslint --ext .ts,.tsx src --fix",
    "lint:prettier": "prettier --check src",
    "lint:prettier:write": "prettier --write src",
    "coverage:wasm:gen": "cd .. && cd quadratic-core && CARGO_INCREMENTAL=0 RUSTFLAGS='-Cinstrument-coverage' LLVM_PROFILE_FILE='coverage/cargo-test-%p-%m.profraw' cargo test",
    "coverage:wasm:html": "cd .. && cd quadratic-core && grcov . --binary-path ./target/debug/deps/ -s . -t html --branch --ignore-not-existing --ignore 'src/wasm_bindings/*' --ignore 'src/bin/*' --ignore '../*' --ignore '/*' -o coverage/html",
    "coverage:wasm:view": "open quadratic-core/coverage/html/index.html",
    "build:javascript:worker": "cd ./src/app/web-workers/javascriptWebWorker/worker/javascript/runner/ && node compileJavascriptRunner.mjs",
    "download:pyodide": "chmod +x ./download-pyodide.sh && ./download-pyodide.sh",
    "circular": "madge --ts-config=tsconfig.json --circular --extensions ts,tsx src/",
    "clean": "npx rimraf node_modules dist build target"
  },
  "browserslist": {
    "production": [
      "defaults and supports es6-module"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^3.3.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
